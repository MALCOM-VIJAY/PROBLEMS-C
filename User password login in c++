#include <iostream>
using namespace std;
int main()
{
    int up=1234,pin,count=0;
    do{
        cout<<"Enter the pin : "<<endl;
        cin>>pin;
        if(pin!=up)
        count++;
    }while(count<3 && (pin!=up));
    if(count<3){
        cout<<"Successfully logged in";
    }
    else{
        cout<<"No more attempts";
    }
    return 0;
}
