#include <stdio.h>

int main()
{
    for(int i=1;i<=5;i++){
        for(int j=1;j<=5;j++){
            printf("%d",j);
        }
        printf("\n");
    }
    return 0;
}
OUTPUT :
12345
12345
12345
12345
12345
***********************************************
1.for(int j=1;j<=i;j++) //change this in inner loop for :
Output:
12
123
1234
12345
*************************************************
2.for(int j=1;j<=i;j++){ 
            printf("%d",i);
        } //change this in inner loop for :
Output:
1
22
333
4444
55555
****************************************************
3.for(int j=1;j<=i;j++){
            printf("%d ",i*j);
        }//change this in inner loop for :
  Output:
1 
2 4 
3 6 9 
4 8 12 16 
5 10 15 20 25 
***********************************************************
REVERSE LOOPS:
#include <stdio.h>

int main()
{
    for(int i=1;i<=5;i++){
        for(int j=5;j>=i;j--){
            printf("%d ",j);
        }
        printf("\n");
    }
    return 0;
}
OUTPUT:
5 4 3 2 1 
5 4 3 2 
5 4 3 
5 4 
5 
***********************************************************
for(int j=i;j<=5;j++){
            printf("%d ",j);
        }
Output:
1 2 3 4 5 
2 3 4 5 
3 4 5 
4 5 
5
***********************************************************
for(int j=i;j>=1;j--){
            printf("%d ",j);
        }
output:
1 
2 1 
3 2 1 
4 3 2 1 
5 4 3 2 1 

