#include <stdio.h>                            

int main()
{
    int o=1;
    while(o<=5){
        int i=1;
        while (i<=5){
            printf("%d",i);
            i++;
            
        }
        printf("\n");
        o++;
    }
    return 0;
}
OUTPUT :
12345
12345
12345
12345
12345
*************************************************************
1. while (i<=o) //change this in inner loop for :
Output:
12
123
1234
12345
***********************************************************
2.while (i<=o) 
  printf("%d",o); //change this in inner loop for :
Output:
1
22
333
4444
55555
*************************************************************
3.while (i<=o){
  printf("%d",i*o); //change this in inner loop for :
  Output:
1 
2 4 
3 6 9 
4 8 12 16 
5 10 15 20 25 
***********************************************************

REVERSE LOOPS:
#include <stdio.h>

int main()
{
    int o=1;
    while(o<=5){
        int i=5;
        while (i>=o){
            printf("%d ",i);
            i--;
        }
        printf("\n");
        o++;
    }
    return 0;
}
OUTPUT:
5 4 3 2 1 
5 4 3 2 
5 4 3 
5 4 
5 
*******************************************************************
int i=o;
        while (i<=5){
            printf("%d ",i);
            i++;
}
Output:
1 2 3 4 5 
2 3 4 5 
3 4 5 
4 5 
5
**********************************************************************
while(o<=5){
        int i=o;
        while (i>=1){
            printf("%d ",i);
            i--;
        }
 Output:
 1 
2 1 
3 2 1 
4 3 2 1 
5 4 3 2 1 

