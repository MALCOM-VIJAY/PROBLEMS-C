//using logic
#include<stdio.h>
int main(){
    int n,r=0;
    scanf("%d",&n);
    while(n>0){
        r = r*10 + n%10;
        n = n/10;

    }
    printf("%d",r);
}
****************************************
//using normal logic:

#include<stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    while(n>0){
        int a;
        a = n%10;
        printf("%d",a);
        n = n/10;
        
    }
    return 0;
}
********************************************
//to understand the logic
#include<stdio.h>
int main(){
    int Input,a,b,c,d,e,f,g,h,i,j,reverse;
    printf("Enter the no :");//12345
    scanf("%d",&Input);
    a = Input/10;//1234
    b = Input%10;//5
    c = a/10;//123
    d = a%10;//4
    e = c/10;//12
    f = c%10;//3
    g = e/10;//1
    h = e%10;//2
    i = g/10;//0
    j = g%10;//1
    reverse = b*(10000) + d*(1000) + f*(100) + h*(10) + j;
    printf ("The final sum is : %d",reverse);
    
    

    return 0;
}
