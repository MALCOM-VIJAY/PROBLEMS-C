//loop
#include <stdio.h>
int main()
{
    int p=0,c=1,d,next;
    scanf("%d",&d);
    printf("%d %d ",p,c);
    for(int i=2;i<d;i++){
        next = p+c;
        printf("%d ",next);
        p=c;
        c= next;
        
    }
    
}
********************************************

//loop
#include <stdio.h>

int main()
{
    int p=0,c=1,d,i=1,next;
    scanf("%d",&d);
    printf("%d %d ",p,c);
    while(i<=d-2){
        next = p+c;
        printf("%d ",next);
        p=c;
        c= next;
        i++;   
    }   
}
